{
  "name": "react-most-type-n-search",
  "version": "0.0.1",
  "description": "",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "extensions": [
            ".es6",
            ".jsx",
            ".js"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production browserify src/app.jsx --extension=.jsx  --extension=.es6| java -jar ../todomvc/bin/compiler.jar > public/app.js",
    "start": "ecstatic -p 8000 public",
    "watch": "watchify -d src/app.jsx --extension=.jsx --extension=.es6 -o public/app.js -dv",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/*"
    ],
    "testFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "jsx"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.0",
    "lodash": "^4.0.0",
    "most": "^1.1.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-most": "^0.6.6",
    "rest": "^1.3.1"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-jest": "^6.0.0",
    "babel-plugin-lodash": "^2.0.1",
    "babel-plugin-transform-react-jsx": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "ecstatic": "^1.3.1",
    "jest-cli": "^0.7.0",
    "uglify-js": "^2.6.1",
    "watchify": "^3.6.1"
  },
  "author": "Jichao Ouyang",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx",
      "lodash"
    ]
  }
}
